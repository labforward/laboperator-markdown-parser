{
  "name": "@laboperator/laboperator-markdown-parser",
  "version": "0.0.1",
  "description": "Extended Markdown parser",
  "homepage": "https://github.com/labforward/laboperator-markdown-parser#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labforward/laboperator-markdown-parser.git"
  },
  "license": "Apache-2.0",
  "author": "labforward GmbH",
  "type": "module",
  "scripts": {
    "build": "del-cli dist && tsc -p tsconfig.json",
    "ci:esm": "node ./bin/test",
    "ci:test": "NODE_OPTIONS=--experimental-vm-modules jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "sort": "sort-package-json ./package.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --watch"
  },
  "dependencies": {
    "hast-util-sanitize": "^4.0.0",
    "lodash": "^4.17.21",
    "mdast-util-to-hast": "^12.2.4",
    "micromark-core-commonmark": "^1.0.6",
    "micromark-factory-space": "^1.0.0",
    "micromark-util-character": "^1.1.0",
    "micromark-util-symbol": "^1.0.1",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@labforward/config": "^0.0.2",
    "@labforward/eslint-config-node": "^0.0.2",
    "@types/jest": "^29.1.2",
    "@types/lodash": "^4.14.186",
    "del-cli": "^5.0.0",
    "eslint": "^8.25.0",
    "jest": "^29.1.2",
    "jest-junit": "^14.0.1",
    "prettier": "^2.7.1",
    "sort-package-json": "^2.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "importSort": {
    ".js, .ts": {
      "parser": "typescript",
      "style": "@labforward/config/import-sort"
    }
  }
}
